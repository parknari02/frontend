{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "functions": {
        "api/proxy.js": {
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/proxy.js"
        }
    ]
}